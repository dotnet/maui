		x:Class="Microsoft.Maui.Controls.Xaml.UnitTests.Bz46921">
	<ContentPage.Resources>
		<ResourceDictionary>
			<Thickness x:Key="thickness0">4,20,4,20</Thickness>
			<Thickness
				x:Key="thickness1"
				Left="4"
				Top="20"
				Bottom="20"
				Right="4"/>

			<Thickness x:Key="thickness2">
				<Thickness.Left>
					4
				</Thickness.Left>
				<Thickness.Top>
					20
