<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="Microsoft.Maui.Controls.Xaml.UnitTests.Bz43694">
	<ContentPage.Resources>
		<ResourceDictionary>
			<x:String x:Key="Dark">Dark</x:String>
			<x:String x:Key="Light">
				<OnPlatform x:TypeArguments="x:String">
					<On Platform="iOS">OpenSans-Light</On>
					<On Platform="Android">OpenSans-Light</On>
					<On Platform="UWP">/Assets/Fonts/OpenSans-Light.ttf#Open Sans</On>
				</OnPlatform>
			</x:String>
		</ResourceDictionary>
	</ContentPage.Resources>
	<Label Text="{StaticResource Light}" 
		VerticalOptions="Center"
		HorizontalOptions="Center"
		x:Name="label"/>
</ContentPage>