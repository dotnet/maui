<?xml version="1.0" encoding="UTF-8"?>
<TabBar 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="using:Microsoft.Maui.Controls.Xaml.UnitTests"
    x:Class="Microsoft.Maui.Controls.Xaml.UnitTests.FontImageExtension">

    <Tab Title="Hello" Icon="{FontImageSource Size={x:Static local:FontImageExtension.Size}, Color={x:Static local:FontImageExtension.Color}, FontFamily={x:Static local:FontImageExtension.FontFamily}, Glyph={x:Static local:FontImageExtension.Glyph}}" />
    <Tab Title="World" Icon="{FontImageSource Size={x:Static local:FontImageExtension.Size}, Color={x:Static local:FontImageExtension.Color}, FontFamily={x:Static local:FontImageExtension.FontFamily}, Glyph={x:Static local:FontImageExtension.Glyph}}" />
    <Tab Title="ContentProperty" Icon="{FontImageSource MyGlyph, Size={x:Static local:FontImageExtension.Size}, Color={x:Static local:FontImageExtension.Color}, FontFamily={x:Static local:FontImageExtension.FontFamily}}" />
    <Tab Title="DefaultSize" Icon="{FontImageSource MyGlyph, Color={x:Static local:FontImageExtension.Color}, FontFamily={x:Static local:FontImageExtension.FontFamily}}" />

</TabBar>
