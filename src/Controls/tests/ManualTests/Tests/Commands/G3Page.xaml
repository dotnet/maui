<?xml version="1.0" encoding="utf-8" ?>
<ContentPage x:Class="Microsoft.Maui.ManualTests.Tests.Commands.G3Page"
             xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="G3Page">
    <ContentPage.MenuBarItems>
        <MenuBarItem Text="Pages">
            <MenuFlyoutItem Command="{Binding GoBackCommand}"
                            Text="Go Back" />
        </MenuBarItem>
    </ContentPage.MenuBarItems>

    <VerticalStackLayout>
        <Label Text="1. Click on the 'Pages' menu bar item." />
        <Label Text="2. Click on the 'Go Back' menu flyout item." />
        <Label Text="3. The test fails if you are not able to navigate back the the previous screen using the 'Go Back' command." />
    </VerticalStackLayout>
</ContentPage>
