<?xml version="1.0" encoding="UTF-8"?>
<controls:TestContentPage xmlns:controls="clr-namespace:Maui.Controls.Sample.Issues"
                          xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          x:Class="Maui.Controls.Sample.Issues.Issue21814FirstPage">
  <VerticalStackLayout
    Padding="12">
    <Button
      x:Name="NavigateButton"
      AutomationId="NavigateButton"
      Text="Navigate to Second Page"
      Clicked="OnNavigateButtonClicked"/>
    <Label 
      Text="NavigatedToEventArgs"
      FontAttributes="Bold"/>
    <Label 
      x:Name="OnNavigatedToLabel"
      AutomationId="OnNavigatedToLabel"/>
    <Label 
      Text="NavigatingFromEventArgs"
      FontAttributes="Bold"/>
    <Label 
      x:Name="OnNavigatingFromLabel"
      AutomationId="OnNavigatingFromLabel"/>
    <Label 
      Text="NavigatedFromEventArgs"
      FontAttributes="Bold"/>
    <Label 
      x:Name="OnNavigatedFromLabel"
      AutomationId="OnNavigatedFromLabel"/>
  </VerticalStackLayout>
</controls:TestContentPage>
