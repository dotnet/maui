<?xml version="1.0" encoding="UTF-8"?>
<controls:TestContentPage xmlns:controls="clr-namespace:Maui.Controls.Sample.Issues"
                          xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                          xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                          x:Class="Maui.Controls.Sample.Issues.Issue21814SecondPage">
  <VerticalStackLayout
  Padding="12">
    <Button
    x:Name="NavigateBackButton"
    AutomationId="NavigateBackButton"
    Text="Navigate Back"
    Clicked="OnNavigateBackButtonClicked"/>
    <Label 
      Text="NavigatedToEventArgs"
      FontAttributes="Bold"/>
    <Label 
      x:Name="OnNavigatedToLabel"
      AutomationId="OnNavigatedToLabel"
      Text="-"/>
    <Label 
      Text="NavigatingFromEventArgs"
      FontAttributes="Bold"/>
    <Label 
      x:Name="OnNavigatingFromLabel"
      AutomationId="OnNavigatingFromLabel"
      Text="-"/>
    <Label 
      Text="NavigatedFromEventArgs"
      FontAttributes="Bold"/>
    <Label 
      x:Name="OnNavigatedFromLabel"
      AutomationId="OnNavigatedFromLabel"
      Text="-"/>
  </VerticalStackLayout>
</controls:TestContentPage>
