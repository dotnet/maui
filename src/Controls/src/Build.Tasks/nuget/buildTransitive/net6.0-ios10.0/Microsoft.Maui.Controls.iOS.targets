  <PropertyGroup>
    <MtouchLink Condition="'$(MtouchLink)' == '' and '$(Configuration)' == 'Debug' and '$(UseInterpreter)' == 'true' and '$(UseMonoRuntime)' == 'true'">None</MtouchLink>
