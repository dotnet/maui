<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="Microsoft.Maui.Controls.Compatibility.ControlGallery.Issues.Issue2288">
	<ContentPage.ToolbarItems>
		<ToolbarItem Text="{Binding MainText}" Command="{Binding MainTextCommand}" />
		<ToolbarItem Text="{Binding MainText}" Command="{Binding MainTextCommand}" Order="Secondary"/>
	</ContentPage.ToolbarItems>
	<StackLayout>
		<Label Text="{Binding MainText}" VerticalOptions="Center" HorizontalOptions="Center" />
	</StackLayout>
</ContentPage>
