<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Target Name="InitInternalTooling" AfterTargets="Restore">
    <Message Text="Installing the workloads" Importance="high" />

    <!-- <MSBuild Projects="$(RepoRoot)src/DotNet/DotNet.csproj"
        Targets="Build" /> -->
         <Exec
        Command="dotnet workload install ios android macos maccatalyst --skip-sign-check --skip-manifest-update --verbosity diag --temp-dir &quot;$(DotNetTempDirectory)&quot; --configfile &quot;$(RepoRoot)NuGet.config&quot;"
        WorkingDirectory="$(RepoRoot)"
        EnvironmentVariables="DOTNET_MULTILEVEL_LOOKUP=0" />
  </Target>

</Project>